<?xml version="1.0"?>

<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>

        <referenceContainer name="base-main-top-container">
            <block class="SolideWebservices\Flexslider\Block\Flexslider" name="flexslider_renewumedispa" template="SolideWebservices_Flexslider::flexslider.phtml">
                <action method="setCode"><argument name="code" xsi:type="string">renewumedispa</argument></action>
                <action method="setStartSlideId"><argument name="start_slide_id" xsi:type="string">1</argument></action>
            </block>
        </referenceContainer>
        <referenceContainer name="content">
            <block class="Scimus\ApiStoreInfo\Block\PromoBlock" name="promo_tabs" template="Magento_Theme::promo_tabs.phtml" />
        </referenceContainer>
	</body>
</page>